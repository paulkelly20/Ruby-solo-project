<h2>List of Genres</h2>
<table class= "tables">
<tr>
<th>Name </th>
<th>Origin </th>
<th>Albums: </th>


<tr/>
<% @genres.each do |genre| %>
<tr>
<td><a href="/genres/<%= genre.id %>"><%= genre.name %></td>
<td><%= genre.origin %></td>

<td>
    <select>
      <% genre.find_albums_by_genre.each do |album| %>
      <option><%= album.title%><% end %></option>
    </select>
    </td>
</tr>
<% end %>
